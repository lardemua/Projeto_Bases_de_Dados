@article{Shen,
abstract = {Injection molding is the most widely used process in manufacturing plastic products. Since the quality of injection molded plastic parts are mostly influenced by process conditions, how to determine the optimum process conditions becomes the key to improving the part quality. In this paper, a combining artificial neural network and genetic algorithm (ANN/GA) method is proposed to optimize the injection molding process. In this method, a BP neural network model is developed to map the complex non-linear relationship between process conditions and quality indexes of the injection molded parts, and a GA is used in the process conditions optimization with the fitness function based on an ANN model. The combining ANN/GA method is used in the process optimization for an industrial part in order to improve the quality index of the volumetric shrinkage variation in the part. The results show that the combining ANN/GA method is an effective tool for the process optimization of injection molding. {\textcopyright} 2006 Elsevier B.V. All rights reserved.},
author = {Shen, Changyu and Wang, Lixia and Li, Qian},
doi = {10.1016/j.jmatprotec.2006.10.036},
file = {:home/sapofree/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Shen, Wang, Li - 2007 - Optimization of injection molding process parameters using combination of artificial neural network and genetic.pdf:pdf},
isbn = {09240136},
issn = {09240136},
journal = {Journal of Materials Processing Technology},
keywords = {Artificial neural network,Genetic algorithm,Numerical simulation,Optimization,Plastic injection molding,molde: processo},
mendeley-tags = {molde: processo},
month = {mar},
number = {2-3},
pages = {412--418},
publisher = {Elsevier},
title = {{Optimization of injection molding process parameters using combination of artificial neural network and genetic algorithm method}},
url = {https://www.sciencedirect.com/science/article/pii/S0924013606008958},
volume = {183},
year = {2007}
}
@article{Ozcelik,
abstract = {In this study, the cell thin shell phone cover produced with polycarbonate/acrylonitrile butadiene styrene (PC/ABS) thermoplastic were decided as a model. First, the effects of the injection parameters on warpage for different thickness values were examined using Taguchi method. The warpage values were found by analyses which were done by moldflow plastic insight (MPI) 4.0 software. The most influential parameter on the warpage of PC/ABS material was found as packing pressure. Second, to determine the forces that cause the plastic part to fail at the points determined over the top surface of the cell phone cover, CATIA V5R12 (general structural analysis) was used. The structural analysis of ABS, PC, reinforced ABS, reinforced PC/ABS thermoplastic materials in addition to PC/ABS material used in telephone manufacture were done in order to determine the performance. When we look at the structural analysis, the strongest materials are 15{\%} carbon fiber reinforced PC/ABS, 15{\%} carbon fiber reinforced ABS, PC, PC/ABS and ABS, respectively. The most critical point on the top surface of the cell phone is point 2},
author = {Ozcelik, Babur and Sonat, Ibrahim},
doi = {10.1016/J.MATDES.2008.04.053},
file = {:home/sapofree/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ozcelik, Sonat - 2009 - Warpage and structural analysis of thin shell plastic in the plastic injection molding.pdf:pdf},
issn = {0261-3069},
journal = {Materials {\&} Design},
keywords = {molde,uso},
mendeley-tags = {molde,uso},
month = {feb},
number = {2},
pages = {367--375},
publisher = {Elsevier},
title = {{Warpage and structural analysis of thin shell plastic in the plastic injection molding}},
url = {https://www.sciencedirect.com/science/article/pii/S0261306908001568?via{\%}3Dihub},
