@article{Trovant1998,
abstract = {The formation of the air gap at the mold⧹metal interface during casting is responsible for considerable changes in local cooling rates. Often, boundary conditions input to a mathematical model have routinely been guessed at (often inaccurately) or have been determined experimentally, both of which can introduce difficulties in predicting solidification history. A novel technique which minimizes the error associated with selecting boundary conditions without experimentation is proposed. Advance knowledge of air gap formation and its correlation to the heat transfer coefficient at the mold⧹metal interface is used to formulate a coupled mathematical model which determines the growth of the air gap and predicts the instantaneous cooling conditions at a given mold wall. The accuracy of the simulation is consequently improved and the need to match experimental boundary conditions is eliminated. This paper presents the current results of an ongoing research program. {\textcopyright} 1998 Canadian Institute of Mining and Metallurgy.},
author = {Trovant, Michael and Argyropolous, Stavros A},
doi = {10.1016/S0008-4433(98)00010-X},
issn = {0008-4433},
journal = {Canadian Metallurgical Quarterly},
keywords = {metal,molde},
mendeley-tags = {metal,molde},
month = {jun},
number = {3-4},
pages = {185--196},
publisher = {No longer published by Elsevier},
title = {{The implementation of a mathematical model to characterize mold metal interface effects in metal casting}},
url = {https://www.sciencedirect.com/science/article/pii/S000844339800010X},
volume = {37},
year = {1998}
}
